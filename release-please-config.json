{
  "packages": {
    ".": {
      "release-type": "simple",
      "changelog-path": "CHANGELOG.md",
      "extra-files": [
        {
          "type": "generic",
          "path": "wp-hoptown-rental/wp-hoptown-rental.php",
          "search": "Version: {version}",
          "replace": "Version: {version}"
        },
        {
          "type": "generic",
          "path": "wp-hoptown-rental/wp-hoptown-rental.php",
          "search": "define( 'HOPTOWN_RENTAL_VERSION', '{version}' );",
          "replace": "define( 'HOPTOWN_RENTAL_VERSION', '{version}' );"
        }
      ]
    }
  }
}
